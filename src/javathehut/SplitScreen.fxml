<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="g_pane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javathehut.con_ss">
  <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="419.0" minWidth="10.0" prefWidth="317.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="351.0" minWidth="10.0" prefWidth="351.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="78.0" minHeight="0.0" prefHeight="22.0" vgrow="SOMETIMES" />
    <RowConstraints vgrow="SOMETIMES" />
    <RowConstraints maxHeight="129.0" minHeight="0.0" prefHeight="32.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <TableView fx:id="tbl_cart" GridPane.columnIndex="2" GridPane.rowIndex="1">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>

      <TableView fx:id="tbl_catalog" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <columns>
          <TableColumn minWidth="100.0" prefWidth="100.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
       
      <Label alignment="CENTER" prefHeight="34.0" prefWidth="211.0" text="Catalog" GridPane.columnIndex="1" />
      <Label alignment="CENTER" prefHeight="33.0" prefWidth="207.0" text="Shopping Cart" GridPane.columnIndex="2" />
   </children>
</GridPane>
